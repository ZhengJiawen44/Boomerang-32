void setup(void) {
  //Create a namespace called "settings" with read/write mode
  pref.begin("settings", false);
  // read the last Last brightness from nvs flash memory
  brightness = 	pref.getInt("brightness", 255);
  analogWrite(5, brightness);
  Serial.begin(115200);
  tft.init();
  pinMode(SENSOR_PIN, INPUT);
  tft.fillScreen(TFT_GREY);
  tft.setCursor(60,60, 2);
  // Set the font colour to be white with a black background, set text size multiplier to 1
  tft.setTextColor(TFT_WHITE);  tft.setTextSize(1);
  // We can now plot text on screen using the "print" class
  //tft.println("Owari da!");
  tft.drawBitmap(0,0,epd_bitmap_____,240,240, TFT_WHITE, TFT_BLACK);
}

void loop() {
  
  tft.setTextColor(TFT_WHITE,TFT_GREY);
  int state = digitalRead(SENSOR_PIN);
  tft.setCursor(60,80, 2);
  float percentage = (brightness/255.0)*100;
  tft.println(String(percentage,1));
  if(state == 1 && (brightness-5)>=0){
    brightness-=5;
    delay(50);
    analogWrite(5, brightness);
  }
  if(brightness==0){
    Serial.println("min brightness");
    isMinBright = true;
  }
  if(brightness==255){
    Serial.println("min brightness");
    isMinBright = false;
  }
  if(isMinBright){
    while(brightness<=255 && state==1 && brightness+5 <=255){
      brightness+=5;
      delay(50);
      analogWrite(5, brightness);
      state = digitalRead(SENSOR_PIN);
      percentage = (brightness/255.0)*100;
      tft.setCursor(60,80, 2);
      tft.println(String(percentage,1));
    }
    
  }
  Serial.println(brightness);
  pref.putInt("brightness", brightness);
}



